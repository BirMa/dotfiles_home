# Quick ssh key output
#<Multi_key> <k> <e> <y> : "https://gist.github.com/name/something"

<Multi_key> <S> <N> <O> <W>     : "❄"
<Multi_key> <p> <l> <a> <n> <e> : "✈"
<Multi_key> <z> <a> <p>         : "⚡"

# prefix 'm' = mathematical symbols
<Multi_key> <m> <d> : "Δ"
<Multi_key> <m> <e> : "ϵ"
<Multi_key> <m> <l> : "λ"

# misc
<Multi_key> <l> <e> <n> <n> <y> : "( ͡° ͜ʖ ͡°)"
<Multi_key> <colon> <bar> <bar> : "ಠ_ಠ"
<Multi_key> <less> <3> : "❤"
